{
  "name": "react-slides",
  "version": "1.0.0",
  "description": "React/Redux slideshow demo",
  "main": "prodServer.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "prod": "node prodServer.js",
    "lint": "eslint src",
    "test": "BABEL_JEST_STAGE=0 jest"
  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-*",
      "./node_modules/redux"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coder36/react-slides.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Mark Middleton <markymiddleton@gmail.com> (http://github.com/coder36)",
  "license": "MIT",
  "homepage": "https://github.com/coder36/react-slides",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.19.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.5.1",
    "file-loader": "^0.8.4",
    "jest": "^0.1.40",
    "jest-cli": "^0.5.10",
    "node-sass": "^3.3.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "rimraf": "^2.4.3",
    "sass": "^0.5.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "express": "^4.13.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.0.1",
    "react-router": "^1.0.0-rc3",
    "react-swipe": "^3.0.0",
    "react-swipeable": "^3.0.2",
    "redux": "^3.0.2",
    "redux-devtools": "^2.1.5",
    "request": "^2.65.0",
    "swipe-js-iso": "^2.0.1"
  }
}
